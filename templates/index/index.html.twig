{% extends 'base.html.twig' %}

{% block body %}
    <div class="container-fluid">
        <div class="row">
            <div class="col-2" style="background-color: #0000FF; color: white; min-height: 25px">
                Hello World: {{ msg }}
            </div>
            <div class="col-10">
                {{ dump(contacts) }}
                {% for key, contact in contacts %}
                    {#key: {{ key }}#}
                    {{ contact.name }}   :::   {{ contact.email }}   :::   {{ contact.number }}
                    <br><br>
                {% endfor %}
            </div>
        </div>
    </div>
    {#{{ parent() }}#}
{% endblock %}